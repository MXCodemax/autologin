{
  "manifest_version": 3,
  "name": "Auto Login",
  "version": "1.0",
  "description": "Extensão para fazer login automático em roteadores.",
  "permissions": ["activeTab", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
     "js": ["content.js"]
    }
  ]
}
